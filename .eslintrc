{
  "extends": ["standard", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "react/prop-types": 0,
    "space-before-function-paren": 0,
    "indent": 0,
    "react/display-name": 0
  },
  "plugins": ["react"],
  "globals": {
    "graphql": false
  }
}
